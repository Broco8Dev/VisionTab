
{
    "name": "VisionTab",
    "description": "A customizable new-tab page based on Apple's VisionOS",
    "version": "1.3",
    "manifest_version": 3,
    "chrome_url_overrides": { "newtab": "index.html" },
    "permissions": ["storage"],
    "content_scripts": [
    {
      "matches": ["chrome://new-tab-page"],
      "js": ["assets/script.js"]
    }
  ]
}